# Example environment variables for Cat AI Factory
# Copy to .env and fill with real values
# NEVER commit .env

# Clawdbot gateway
CLAWDBOT_GATEWAY_TOKEN=changeme

# LLM providers (examples)
OPENAI_API_KEY=changeme
GROK_API_KEY=changeme
GEMINI_API_KEY=changeme
GOOGLE_API_KEY=changeme

# Telegram (optional)
TELEGRAM_BOT_TOKEN=changeme
TELEGRAM_CHAT_ID=changeme
TELEGRAM_ALLOWED_USER_ID=changeme


# PR5 (Planner - Gemini AI Studio)
# Optional override. Example: gemini-2.5-flash
GEMINI_MODEL=

# PR25+ (Vertex / GCP runtime config)
# Use placeholders only in repo.
VERTEX_PROJECT_ID=changeme
VERTEX_LOCATION=changeme
GOOGLE_CLOUD_PROJECT=changeme
GCP_PROJECT_ID=changeme
GCP_PROJECT_NUMBER=changeme
GCP_REGION=changeme

# PR35+ (Wan via Alibaba Cloud Model Studio / DashScope)
# Keep secrets only in local .env, never in git.
DASHSCOPE_API_KEY=changeme
# Set to the DashScope endpoint shown in your Alibaba Cloud console for your account/region.
# Common default is: https://dashscope-intl.aliyuncs.com
WAN_DASHSCOPE_BASE_URL=https://dashscope-intl.aliyuncs.com
# Example model names depend on your account access: wan2.2-t2v / wan2.2-i2v / wan2.6-*
WAN_DASHSCOPE_MODEL=wan2.2-t2v
WAN_DASHSCOPE_POLL_SECONDS=240
WAN_DASHSCOPE_POLL_INTERVAL=5

# PR35+ (ComfyUI adapter lane; optional/local service)
COMFYUI_BASE_URL=http://127.0.0.1:8188
COMFYUI_WORKFLOW_ID=caf_dance_loop_v1

# PR35+ (Lab adapters; optional, lab-mode only)
SORA_LAB_API_KEY=changeme
SORA_LAB_MODEL=sora-2.0-lab-stub
META_AI_LAB_API_KEY=changeme
META_AI_LAB_BASE_URL=
META_AI_LAB_MODEL=meta-movie-gen-lab-stub

# Engine routing mode
# production (default, deterministic baseline) | lab (challenger/advisory experiments)
CAF_ENGINE_ROUTE_MODE=production
# Guarded authority trial for advisory routing (0/1). Keep disabled by default.
CAF_QC_AUTHORITY_TRIAL=0
