# Example environment variables for Cat AI Factory
# Copy to .env and fill with real values
# NEVER commit .env

# Clawdbot gateway
CLAWDBOT_GATEWAY_TOKEN=changeme

# LLM providers (examples)
OPENAI_API_KEY=changeme
GEMINI_API_KEY=changeme
GOOGLE_API_KEY=changeme

# Telegram (optional)
